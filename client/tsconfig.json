{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "strict": true,
    "jsx":"preserve",
    "importHelpers": true,
    "moduleResolution": "node",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "isolatedModules": true,
    "sourceMap": true,
    "typeRoots": ["node_modules/@types"],
    "lib": [
      "esnext",
      "dom",
      "dom.iterable",
      "scripthost"
    ],
    "allowJs": true,
  },
  "include": [
    "**/*.ts","**/*.tsx",".eslintrc.cjs"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}
